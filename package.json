    "cross-env": "^3.2.4",
    "csurf": "^1.9.0",
    "es6-promise": "^4.1.0",
    "express": "^4.15.2",
    "js-cookie": "^2.1.4",
    "jsonwebtoken": "^7.3.0",
    "lru-cache": "^4.0.2",
    "mongodb": "^2.2.26",
    "mongoose": "^4.9.5",
